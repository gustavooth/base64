{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(Linux) Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/bin/base64",
      "args": ["man"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/bin",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
      //"preLaunchTask": "Build All"
    }
  ]
}